{
    "manifest_version": 3,
    "name": "Image logo detection",
    "version": "1.0",
    "description": "A simple browser extension example",
    "permissions": ["activeTab"],
    "action": {
      "default_icon": {
        "16": "images/icon48.png",
        "48": "images/icon16.png",
        "128": "images/icon128.png"
      },
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  